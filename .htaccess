Options +FollowSymLinks -MultiViews
# Turn mod_rewrite on
RewriteEngine On
RewriteBase /
RewriteCond %{DOCUMENT_ROOT}/public_html/$1 -f [OR]
RewriteCond %{DOCUMENT_ROOT}/public_html/$1 -d [OR]
RewriteCond %{DOCUMENT_ROOT}/public_html/$1 -l
RewriteRule (?!^public_html/)^(.*)$ /public_html/$1 [R=302,L,NC]